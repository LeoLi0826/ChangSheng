    <table name="NumericValueCheckConfigCategory" value="NumericValueCheckConfig" input="..\..\..\..\cn.etetet.yiuinumericconfig\Luban\Config\Datas\*@NumericValueCheck.json" output="NumericValueCheckConfigCategory"/> 

